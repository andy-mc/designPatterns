{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "max-classes-per-file": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "no-empty-function": "off",
    "getter-return": "off"
  }
}
